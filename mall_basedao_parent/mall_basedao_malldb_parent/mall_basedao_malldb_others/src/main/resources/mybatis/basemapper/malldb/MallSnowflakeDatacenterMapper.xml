<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper
	namespace="com.yunyihenkey.basedao.malldb.basemapper.MallSnowflakeDatacenterMapper">

	<select id="selectByMacAddress" parameterType="java.lang.String"
		resultType="map">
		SELECT
		*
		FROM
		mall_snowflake_datacenter
		WHERE
		mac_address
		=#{macStr}
		LIMIT 1
	</select>


</mapper>